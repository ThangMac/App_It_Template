# Uncomment the next line to define a global platform for your project
# platform :ios, '9.3'

use_frameworks!

def shared_pods
    pod 'Moya'
    pod 'SwiftyJSON'

    # Loading effect
    pod 'IQKeyboardManagerSwift'

    pod 'SwiftDate'
    pod 'MBProgressHUD', '~> 1.1.0'

    pod 'SDWebImage'
    pod 'SVPullToRefresh'

    # CollectionViewAlign
    pod 'UICollectionViewLeftAlignedLayout'

    # RatingStar
    pod 'Cosmos', '~> 21.0'

    # XLPagerTabStrip
    pod 'XLPagerTabStrip'

    # GoogleMap
    pod 'GoogleMaps'
    pod 'GooglePlaces'

    # GoogleAds
    pod 'Google-Mobile-Ads-SDK'

    # Firebase
    pod 'Firebase/Messaging'
    pod 'Firebase/Core'
    
end

target 'App_It_Template' do
    shared_pods
end